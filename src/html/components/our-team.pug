//- 
include ../mixins/svg

//- 
section.our-team.py-5

    .our-team__heading.text-center
        h3.text-mid.text-light-1.weight-bolder.text-uppercase our team
        p.text-xsmall.text-light-2.width-40.mx-auto.first-letter talented employees of our company will be able to solve any problem in no matter the complexity and timing of execution







    //- 
    .row.no-gutters.our-team__wrapper.container-section.p-0.text-light-1.mt-4

        .col.our-team__box.position-relative.overflow-hidden
            img(class="img-fluid" src="../img/our-team/1.jpg", alt="1")
            .our-team__info.position-absolute.d-flex.flex-column.align-items-center.justify-content-center.text-capitalize
                p.our-team__info__name.text-uppercase.weight-bolder vctoria berger
                p.our-team__info__job manager
                .our-team__info__icons.my-3
                    a(href="https://www.google.com" target="_blank")
                        +getSvgIcon('Facebook', 'resize-footer-svg grow-0 shrink-0')
                    a(href="https://www.google.com" target="_blank")
                        +getSvgIcon('Twitter', 'resize-footer-svg grow-0 shrink-0')
                    a(href="https://www.google.com" target="_blank")
                        +getSvgIcon('Instagram', 'resize-footer-svg grow-0 shrink-0')
                    a(href="https://www.google.com" target="_blank")
                        +getSvgIcon('Linkedin', 'resize-footer-svg grow-0 shrink-0')
                p.our-team__info__phone 8 495 999 00 99
                p.our-team__info__email vikaberger@gmail.com

